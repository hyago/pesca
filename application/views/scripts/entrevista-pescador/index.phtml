            </fieldset>
            <div id="Submit">
                <input type="submit" value="Cadastrar Ficha"/>
            </div>
            </form> 
        </div>
<div id="accordion">
  <!-- PESCA DE REDE --------------------------------------------------------------------------------------->
  <h3>Pesca de Rede</h3>
    <div>
       <div id="esquerda-entrevista">
        <form id="formulario_Entrevista" action="" method="post">
              <fieldset id="Desembarque">
                
                <div id="form-esq">
                    <label for="tipo">Pesqueiro:</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
                <div id="form-esq">
                    Tipo de Pesca:
                    <select name="Tipo_Pesca[]" >
                        <option value="" selected="selected">Tipo de Pesca</option>
                        <option value="Vara de Mão">Emalhe</option>
                        <option value="Tarrafa">Calão</option>
                        <option value="Rede de Emalhe">Tarrafa</option>        
                    </select>
                </div>
                <div id="form-esq">
                    <label for="tipo">Outra, Qual?:</label>
                    <input type="text" id="artePescaLinha" name="OutraartePescaLinha"/>
                    <br/>    
                </div>
                <div id="form-esq">
                    <label for="tipo">Destino</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/> 
                </div>
                <div id="form-esq">
                    <label for="tipo">Diesel(L):</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
                <div id="form-esq">
                    <label for="tipo">Óleo(L):</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
                <div id="form-esq">
                    <label for="tipo">Alimento(R$):</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
                <div id="form-esq">
                    <label for="tipo">Gelo(R$):</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
                <div id="form-esq">
                    <label for="tipo">Avistamento:</label>
                    <input type="text" id="nome_pesqueiro" name="nome_pesqueiro_pescaLinha"/>
                </div>
       </div> 
                <fieldset id="EspecieCapturada">
                    <td><label for="tipo">Quantidade de Peixes Capturadas:</label></td>
                    <table style="border:none">
                        <tr class="linhas">
			<br/>
                        <td>
                            <select name="Especie[]" id="Especie">
                                <option value="" selected="selected">Espécie</option>
                            </select>
			</td>
                        <td>
                            <label>Quantidade</label>
                            <input type="text" name="qtd_peixes[]" id="numeric"/>
                        </td>
                        <td>
                            <label for="tipo">Peso (Kg):</label>
                            <input type="text" id="numeric" name="peso_pescaLinha"/>           
                        </td>
                        <td>
                            <label for="tipo">Preço por Kg:</label>
                            <input type="text" id="numeric" name="preco_pescaLinha"/>           
                        </td>
			<td>
                            <a  class="removerCampo" title="Remover linha">Remove Campo</a>
                        </td>
			</tr>
                    </table>
                    <br/>
                <a class="adicionarCampo" title="Adicionar item">Add Campo</a>
                </fieldset>
                     
            <div id="Submit">
                <input type="submit" value="Enviar"/>
            </div>
        </form>
       
    </div>

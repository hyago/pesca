                <input type="text" id="arte_pesca" name="arte_pesca" placeholder="Arte de Pesca"/><br />	
              </div>
              
              <div id="direita">
		<legend><h4>Embarcação</h4></legend>
			<div id="form-esq">
                            <label>Barco:</label>
                            <input type="text" id="nome" name="barco" placeholder="Nome do Barco" /><br />
                        </div>
                        <div id="form-esq">
                            <label>Tipo de Barco:</label>
                            <input type="text" id="nome" name="tipo_barco" placeholder="Tipo de Barco"/><br />
                        </div>
                        <div id="form-esq">    
                            <label >Mestre:</label>
                            <input type="text" id="nome" name="mestre" placeholder="Nome do Mestre"/><br />
                        </div>
                        <div id="form-esq">
                            <label>Qtd de Pescadores:</label>
                            <input type="text" id="numeric" name="num_pescadores" placeholder="Num"/><br />
                        </div>
              </div>
              <div id="form-esq">
                    <label for="tipo">Subamostra?:</label>
                    <input type="checkbox" id="subamostra" name="subamostra" /><br />
              </div>
